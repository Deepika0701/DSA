class Solution {
    long sumOfSeries(long N) {
        // code here
        long sum=0;
        if(N==1)
        return 1;
        else{
        return((N*N*N+sumOfSeries(N-1)));
        }
        
        // long sum=(N*(N+1)/2)*(N*(N+1)/2);
        // return sum;
    }
}
