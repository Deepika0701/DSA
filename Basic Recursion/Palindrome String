https://practice.geeksforgeeks.org/problems/palindrome-string0817/1
class Solution {
    int isPalindrome(String S) {
        // code here
        
        int j=S.length()-1;
        int i=0;
        while(i<j)
        {
            if(S.charAt(i)!=S.charAt(j))
            return 0;
            else
            {
                i++;
                j--;
            }
        }
        return 1;
    }
};
