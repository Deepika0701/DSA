
https://practice.geeksforgeeks.org/problems/is-binary-number-multiple-of-30654/1
//User function Template for Java

class Solution {
    int isDivisible(String s) {
        
    int sum = 0;
    for (int i = 0; i < s.length(); i++) {
        if (s.charAt(i) == '1') {
            if (i % 2 == 0) {
                sum += 1;
            } else {
                sum += 2;
            }
        }
    }
       if(sum%3==0)
       return 1;
       else 
       return 0;
    }
}
